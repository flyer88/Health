<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.holyboom.flyer.health" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".HealthApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activity.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity"></activity>
        <activity android:name=".Activity.RegisterActivity"></activity>
        <activity android:name=".Activity.EditEncylopediaActivity"></activity>
        <activity android:name=".Activity.PublishedEncylopediaActivity"></activity>
        <activity android:name=".Activity.PatientQestionActivity"></activity>
        <activity android:name=".Activity.PatientRequestActivity"></activity>
        <activity android:name=".Activity.DoctorInfoActivity"></activity>
        <activity android:name=".Activity.AddMettingActivity"></activity>
        <activity android:name=".Activity.EditPatientHeadActivity"></activity>
        <activity android:name=".Activity.EditDoctorHeadActivity"></activity>
        <activity android:name=".Activity.AddDocumentActivity"></activity>
    </application>

</manifest>
